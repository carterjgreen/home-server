          "hide": false,
          "query": "import \"timezone\"\r\n\r\nfrom(bucket: \"energy\")\r\n  |> range(start: v.timeRangeStart, stop:v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"bidgely\" or r[\"_measurement\"] == \"hydro_ottawa\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"usage\")\r\n  |> drop(columns: [\"_measurement\"])\r\n  |> aggregateWindow(every: 1d, fn: sum,  location: timezone.location(name: \"America/Toronto\"))\r\n  |> timeShift(duration: -1d)",
          "refId": "C"
        },
